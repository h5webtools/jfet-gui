{
  "name": "jfet-gui",
  "version": "1.0.0",
  "description": "",
  "main": "./src/main/index.js",
  "scripts": {
    "dev": "jfet build -w & jfet server -l -c ./src/renderer/public",
    "build": "cross-env NODE_ENV=production jfet build",
    "start": "electron .",
    "pack": "build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "http://git.jtjr.com/h5_webtools_grp/jfet-gui.git"
  },
  "keywords": [],
  "author": "canye.wu@jyblife.com",
  "license": "MIT",
  "devDependencies": {
    "@jyb/eslint-config-jyb": "^1.0.1",
    "electron-builder": "^20.19.2",
    "eslint": "^4.7.1",
    "nodeinstall": "^0.1.3",
    "electron": "^2.0.3"
  },
  "dependencies": {
    "fs-extra": "^6.0.1",
    "node-pty": "^0.7.6",
    "npm-run-path": "^2.0.2"
  },
  "jfetOptions": {
    "configFilePath": "src/renderer"
  },
  "build": {
    "productName": "jfet-gui",
    "appId": "com.jyblife.jfet",
    
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "static/icon"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "static/icon"
    },
    "linux": {
      "target": [
        "deb"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  }
}
